<editor>
  <formElement>
    <label>Configuration:</label>
    <property>config</property>
    <type>entry</type>
    <required>1</required>
    <documentation>Name of the plugin configuration to use to connect to Subject7</documentation>
    <value></value>
  </formElement>

  <formElement>
    <label>Test Category:</label>
    <property>testCategory</property>
    <type>select</type>
    <required>0</required>
    <documentation>Test category (used by the DevOps Insight server reports)</documentation>
    <option><name>system-test</name><value>system-test</value></option>
    <option><name>unit-test</name><value>unit-test</value></option>
    <option><name>smoke-test</name><value>smoke-test</value></option>
    <option><name>load-test</name><value>load-test</value></option>
    <value>system-test</value>
  </formElement>

  <formElement>
    <label>Execution ID:</label>
    <property>executionId</property>
    <type>entry</type>
    <required>1</required>
    <documentation>Id of the execution to get data for</documentation>
    <value></value>
  </formElement>

  <formElement>
    <label>Metadata property path:</label>
    <property>metadataPropertyPath</property>
    <type>entry</type>
    <documentation>Property sheet where run metadata will be stored. Optional field. If omitted, /myJob/subject7-%executionId% will be used.</documentation>
    <required>0</required>
   </formElement>

</editor>
