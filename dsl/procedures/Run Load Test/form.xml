<editor>
  <formElement>
    <type>entry</type>
    <label>Configuration:</label>
    <property>config</property>
    <required>1</required>
    <documentation>Name of the plugin configuration to use to connect to Subject7</documentation>
    <value></value>
  </formElement>
  <formElement>
    <type>entry</type>
    <label>Load Plan Name:</label>
    <property>loadPlanName</property>
    <required>1</required>
    <documentation>Name of the load plan test to run</documentation>
    <value></value>
  </formElement>
  <formElement>
    <type>entry</type>
    <label>Pool:</label>
    <property>pool</property>
    <required>0</required>
    <documentation>Subject7 pool to run tests on</documentation>
    <value></value>
  </formElement>
  <formElement>
    <type>entry</type>
    <label>Test Case Name:</label>
    <property>testCaseName</property>
    <required>0</required>
    <documentation>Name of the test case to run</documentation>
    <value></value>
  </formElement>
  <formElement>
    <type>entry</type>
    <label>Data Sets:</label>
    <property>dataSet</property>
    <required>0</required>
    <documentation>List of data sets to run, in this format: template_name_1=data_set_1; template_name_2=data_set_2, data_set_3 ...</documentation>
    <value></value>
  </formElement>
</editor>
